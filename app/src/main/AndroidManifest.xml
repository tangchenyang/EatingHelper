<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/eating"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EatingHelper">
        <activity 
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".activity.BreakfastMenuActivity" 
            android:exported="false" />
        <activity 
            android:name=".activity.OnePersonMenuActivity" 
            android:exported="false" />
        <activity 
            android:name=".activity.TwoPersonMenuActivity" 
            android:exported="false" />
        <activity 
            android:name=".activity.FourPersonMenuActivity" 
            android:exported="false" />
        <activity 
            android:name=".activity.AddFoodActivity" 
            android:exported="false" />
        <activity 
            android:name=".activity.AllFoodsMenuActivity" 
            android:exported="false" />
    </application>

</manifest>