<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eatinghelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/eating"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EatingHelper">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BreakfastMenuActivity" />
        <activity android:name=".activity.OnePersonMenuActivity" />
        <activity android:name=".activity.TwoPersonMenuActivity" />
        <activity android:name=".activity.FourPersonMenuActivity" />
        <activity android:name=".activity.AddFoodActivity" />
        <activity android:name=".activity.AllFoodsMenuActivity" />
    </application>

</manifest>